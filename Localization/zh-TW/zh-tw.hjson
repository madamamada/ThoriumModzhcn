			BlindingDash: {
				DisplayName: 致盲冲刺
				Description: 向前冲刺，眩晕路径上的敌怪
			}

			BloodLotus: {
				DisplayName: 绽血莲华
				Description:
					'''
					原地静止，向最近的敌怪释放刀刃洪流
					每个刀刃命中时都会为你回复10%伤害的生命值
					'''
			}

			CobraBite: {
				DisplayName: 蛇牙噬咬
				Description:
					'''
					掷出一团剧毒蛇牙，对命中的敌怪施加酸性毒液
					若敌人受到中毒或酸性毒液影响，则蛇牙必定暴击
					'''
			}

			Decoy: {
				DisplayName: 引敌诱饵
				Description:
					'''
					召唤一个敌怪诱饵
					诱饵会试图吸引附近敌怪的注意力
					'''
			}

			GravityWell: {
				DisplayName: 引力陷阱
				Description: 在光标位置召唤引力井，吸引附近的敌怪
			}

			HiddenBlade: {
				DisplayName: 隐匿之刃
				Description: 向光标方向掷出一枚锐利的袖刃
			}

			LastStand: {
				DisplayName: 背水一战
				Description:
					'''
					受到致命伤害时自动激活，获得{0}{^0:秒;seconds}的无敌
					无敌期间会提升自身{1}%投掷速度和伤害但结束时会死亡
					'''
			}

			MeteorStomp: {
				DisplayName: 流星践踏
				Description:
					'''
					将自己下砸一段距离，击退过程中的敌怪
					落在物块上则会免疫摔落伤害
					'''
			}

			StickyExplosive: {
				DisplayName: 粘性炸药
				Description: 掷出一个粘性炸药，在短暂延迟后爆炸
			}

			ShadowClone: {
				DisplayName: 暗影克隆
				Description: 召唤一个影分身迅速攻击附近的敌怪
			}

			ShadowDance: {
				DisplayName: 暗影之舞
				Description: 将你传送至光标处
			}

			SmokeBomb: {
