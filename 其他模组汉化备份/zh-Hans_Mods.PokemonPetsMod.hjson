Configs: {
	Config: {
		//[i:PokemonPetsMod/CosmogItem] or [i:48] prints an emote of an item onto the label
		
		DisplayName: 设置

		Headers: {
			PetSummonItemAcquisition: "[i:48] 宠物召唤物获取"
			PetSettings: "[i:509] 宠物设置"
		}

		CosmogToggle: {
			Label: "[i:PokemonPetsMod/CosmogItem] 切换科斯莫古"
			Tooltip:
				'''
				如果启用，科斯莫古的宠物召唤物，在困难模式下旅行商人处购买
				如果科斯莫古被设置为照明宠物，该召唤物在世纪之花击败后出售
				如果禁用，则不可获取
				'''
		}

		ArchenToggle: {
			Label: "[i:PokemonPetsMod/ArchenItem] 切换始祖小鸟"
			Tooltip:
				'''
				如果启用，始祖小鸟的召唤物，通过提炼机提炼泥沙块，雪泥块或沙漠化石获取
				如果禁用，则不可获取
				'''
		}

		GlaceonToggle: {
			Label: "[i:PokemonPetsMod/GlaceonItem] 切换冰伊布"
			Tooltip:
				'''
				如果启用，冰伊布的召唤物，击败冰雪巨人获得
				如果禁用，则不可获取
				'''
		}

		HoppipToggle: {
			Label: "[i:PokemonPetsMod/HoppipItem] 切换毽子草"
			Tooltip:
				'''
				如果启用，毽子草的召唤物，击败食人怪和愤怒捕手获得
				如果禁用，则不可获取
				'''
		}

		PorygonToggle: {
			Label: "[i:PokemonPetsMod/PorygonItem] 切换多边兽"
			Tooltip:
				'''
				如果启用后，多边兽的召唤物，击败火星人入侵时获得
				如果禁用，则不可获取
				'''
		}

		MagikarpToggle: {
			Label: "[i:PokemonPetsMod/MagikarpItem] 切换鲤鱼王"
			Tooltip:
				'''
				如果启用，鲤鱼王的召唤物，打开木箱和珍珠木箱获得
				如果禁用，则不可获取
				'''
		}

		GastlyToggle: {
			Label: "[i:PokemonPetsMod/GastlyItem] 切换鬼斯"
			Tooltip:
				'''
				如果启用，鬼斯的召唤物，击败死神{日蚀}获得
				如果禁用，则不可获取
				'''
		}

		GastlyPetSettings: {
			Label: 鬼斯设置
			Tooltip:
				'''
				切换鬼斯当做宠物还是照明宠物
				如果鬼斯被设置为照明宠物，它将发出光辉相当于妖灵瓶。
				默认为照明宠物(推荐)
				'''
		}

		CosmogPetSettings: {
			Label: 科斯莫古设置
			Tooltip:
				'''
				切换科斯莫古当做宠物还是照明宠物
